{
    "name": "code-lab-eu/real_estate",
    "description": "Drupal distribution for real estate websites.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Pieter Frenssen",
            "email": "pieter@frenssen.be"
        }
    ],
    "require": {
        "php": "^7.3",
        "drupal/core": "~9"
    },
    "require-dev": {
        "drupal/core-dev": "~9",
        "drush/drush": "~10",
        "zaporylie/composer-drupal-optimizations": "~1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2.32"
        }
    },
    "autoload": {
        "files": ["load.environment.php"]
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "./vendor/bin/robo environment:setup-basic-auth"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/LoversOfBehat/ScreenshotExtension.git"
        }
    ],
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
